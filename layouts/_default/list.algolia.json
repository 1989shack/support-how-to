{{/* Generates a valid Algolia search index */}}
{{- $.Scratch.Add "index" slice -}}
{{- $section := $.Site.GetPage "section" .Section }}
{{- range .Site.AllPages -}}
    {{- $.Scratch.Add "index" (dict "audit_date" .Audit_date "created_date" .Created_date "created_by" .Created_by "title" .Title "type" .Type "last_modified_by" .Last_modified_by "last_modified_date" .Last_modified_date "product" .Product "product_url" .Product_url "category" .Category )}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}